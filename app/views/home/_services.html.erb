<!-- SERVICES -->
<section id="services">
  <div class="container">

    <div class="row">


      <div class="col-md-12 artisan art32">


        <% @services.each do |service| %>

            <div class="col-md-4 col-sm-4" style="text-align: center">

              <em class="<%= service.service_icon %>" style="font-size: 48px;"></em><br>
              <span class="ss32"><%= service.name %></span>

            </div>

        <% end %>

      </div>
    </div>

  </div>

</section>
